{
  "rewrites": [
    {
      "source": "/amp",
      "destination": "/amp.html"
    }
  ],
  "redirects": [
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "petir33-landing.vercel.app"
        }
      ],
      "destination": "https://petir33.asia/:path*",
      "permanent": true
    }
  ]
}
